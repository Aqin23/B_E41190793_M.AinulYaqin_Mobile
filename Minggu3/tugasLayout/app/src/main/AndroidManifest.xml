<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.tugasLayout">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.PraktikumLayout">

        <activity android:name="com.example.tugasLayout.ScrollViewHorizontal" />
        <activity android:name="com.example.tugasLayout.ScrollView" />
        <activity android:name="com.example.tugasLayout.MaterialDesign" />
        <activity android:name="com.example.tugasLayout.TableLayout" />
        <activity android:name="com.example.tugasLayout.FrameLayout" />
        <activity android:name="com.example.tugasLayout.ConstraintLayout" />
        <activity android:name="com.example.tugasLayout.RelativeLayout" />
        <activity android:name="com.example.tugasLayout.LinearLayout" />
        <activity android:name="com.example.tugasLayout.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>